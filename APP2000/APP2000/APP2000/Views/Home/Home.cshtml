@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>Valg 2021</h1>
    <p class="Dette er hjemmesiden til valgsapplikasjonen. Her finner du Forskjellig innformasjon om valget.
       under finner du en snarvei for å logge inn hvis du har en bruker lagret"></p>
    <p><a href="/Views/Home/Login.cshtml" class="btn btn-primary btn-lg">Logg inn</a></p>
</div>

<div class="row">
